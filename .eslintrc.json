{
	"extends": "airbnb-base",
	"env": {
		"browser": true,
		"mocha": true,
		"jquery": true
	},
	"rules": {
		"no-tabs": [
			"off"
		],
		"no-console": "error",
		"no-alert": ["off"],
		"indent": [
			"error",
			"tab"
		],
		"quotes": [
			"error",
			"double"
		],
		"comma-dangle": [
			"error",
			"only-multiline",
			{
				"functions": "never"
			}
		]
	}
}